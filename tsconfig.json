{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "DOM",
      "ES6",
      "DOM.Iterable",
      "ScriptHost",
      "ES2016.Array.Include"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "include": [
    "src",
    "basket/otladkaRoutesOld.ts",
    "basket/MapRouteBindFormIn.tsx",
    "basket/MapCreateVertexError.tsx",
    "src/components/MapComponents/MapWaysFormaMain.tsx",
    "src/components/MapComponents/MapRouteBindForm.tsx",
    "public/!coordinatesReducer.tsx",
    "public/!massplanReducer.tsx",
    "public/!interfacePlans.d.ts",
    "public/!mapReducer.tsx",
    "public/!interfaceRoute.d.ts",
    "public/otladkaRoutes.ts",
    "public/otladkaPlans.ts",
    "public/otladkaMaps.ts",
    "public/!interfaceMAP.d.ts"
  ]
}
